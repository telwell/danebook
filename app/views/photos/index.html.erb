<div id="wrapper" class="container-fluid">
<%= render partial: '/shared/main_navbar' %>

<div id="content" class="col-sm-10 col-md-offset-1">

	<%= render partial: '/shared/header' %>

	<main class="row">

		<!-- Photos -->
		<div class="col-sm-12 all-friends-container">
			<% @photos.each do |photo| %>
				
				<!-- QUESTION: This is obviously not a good naming structure...
							is there a way I can change this or am I stuck for now?-->
				<%= image_tag photo.photo.url(:medium) %>

			<% end %>
			<%= link_to "Add New Photo", new_profile_photo_path(@profile), class: "btn btn-primary" %>
		</div><!-- / Photos -->

	</main>	<!-- /row -->

</div> <!-- / content -->